@prefix : <#>.
@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix tes: <./>.
@prefix c: </profile/card#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

:owner
    acl:accessTo tes:;
    acl:agent c:me;
    acl:defaultForNew tes:;
    acl:mode acl:Control, acl:Read, acl:Write.
:public
    acl:accessTo tes:;
    acl:agentGroup <group-listing.ttl#folks>;
    acl:defaultForNew tes:;
    acl:mode acl:Read.
